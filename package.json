{
  "name": "shire-reckoning",
  "version": "5.0.0-alpha.4",
  "homepage": "https://psarando.github.io/shire-reckoning",
  "source": "src/lib.js",
  "main": "lib/TolkienCalendars.js",
  "keywords": [
    "calendar",
    "date",
    "Tolkien",
    "Shire",
    "Rivendell",
    "Imladris",
    "Gondor",
    "Numenor",
    "lord-of-the-rings",
    "middle-earth"
  ],
  "license": "EPL-1.0",
  "files": [
    "lib",
    "src/lib.js",
    "src/ui",
    "src/ShireReckoning.js",
    "src/RivendellReckoning.js",
    "src/GondorReckoning.js",
    "src/Utils.js",
    "README.md",
    "Middle-earth-simulation.md",
    "Rivendell_Drift.md",
    "Kings_Reckoning_Rules_and_Deficit.md",
    "CHANGELOG.md"
  ],
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@btmills/prettier": "^1.17.0",
    "@storybook/addon-storysource": "^6.5.12",
    "@storybook/addons": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.12",
    "@storybook/theming": "^6.5.12",
    "autoprefixer": "^10.4.14",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "mini-css-extract-plugin": "^2.7.4",
    "postcss-loader": "^7.1.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "webpack": "^5.76.1",
    "webpack-cli": "^5.0.1"
  },
  "peerDependencies": {
    "react": "^16.8.6 || ^17.0.0",
    "react-dom": "^16.8.6 || ^17.0.0"
  },
  "scripts": {
    "start": "npm run storybook",
    "storybook": "start-storybook -p 9009",
    "build-storybook": "build-storybook --output-dir build/examples",
    "build-app": "webpack --mode=production --node-env=production --config webpack.app.config.js",
    "build": "webpack --mode=production --node-env=production",
    "test": "jest --watch",
    "prettier": "prettier --write 'src/**/*.js'"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "postcss": {
    "plugins": [
      "autoprefixer"
    ]
  },
  "resolutions": {
    "@storybook/react/webpack": "^5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/src/__mocks__/styleMock.js"
    }
  },
  "prettier": {
    "tabWidth": 4,
    "trailingComma": "es5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
